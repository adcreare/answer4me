{
  "name": "answer-4-me",
  "version": "0.0.1",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "export AWS_REGION=us-east-1 && export AWS_ACCESS_KEY_ID=no && export AWS_SECRET_ACCESS_KEY=no && jest"
  },
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/jest": "^23.3.1",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.5.8",
    "@types/request-promise-native": "^1.0.15",
    "@types/winston": "^2.3.9",
    "assert": "^1.4.1",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "nock": "^9.6.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5"
  },
  "author": "Adcreare",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.291.0",
    "mp3-duration": "^1.1.0",
    "request-promise-native": "^1.0.5",
    "twilio": "^3.19.1",
    "winston": "^3.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
